<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/currentDesc"
        android:layout_width="106dp"
        android:layout_height="35dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="13dp"
        android:layout_marginBottom="14dp"
        android:text="currentDesc"
        app:layout_constraintBottom_toTopOf="@+id/currentDateTimeEpoch"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/address" />

    <TextView
        android:id="@+id/currentDateTimeEpoch"
        android:layout_width="107dp"
        android:layout_height="36dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="76dp"
        android:text="currentDateTimeEpoch"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/currentDesc" />

    <TextView
        android:id="@+id/address"
        android:layout_width="107dp"
        android:layout_height="35dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="52dp"
        android:layout_marginBottom="13dp"
        android:text="address"
        app:layout_constraintBottom_toTopOf="@+id/currentDesc"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.68" />

    <Button
        android:id="@+id/dayActivityButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="78dp"
        android:layout_marginTop="94dp"
        android:layout_marginEnd="239dp"
        android:layout_marginBottom="201dp"
        android:onClick="seconActivity"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/currentDateTimeEpoch"
        app:layout_constraintVertical_bias="1.0" />
</androidx.constraintlayout.widget.ConstraintLayout>